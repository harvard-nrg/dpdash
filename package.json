{
  "name": "po",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack && npm run transpile",
    "transpile": "npx babel ./server --out-dir dist",
    "start": "NODE_ENV=production ./node_modules/pm2/bin/pm2 start dist/bin/www.js",
    "monitor": "./node_modules/pm2/bin/pm2 monit",
    "stop": "./node_modules/pm2/bin/pm2 stop all && ./node_modules/pm2/bin/pm2 delete all",
    "logs": "./node_modules/pm2/bin/pm2 logs --lines 100",
    "restart": "NODE_ENV=production ./node_modules/pm2/bin/pm2 stop all && ./node_modules/pm2/bin/pm2 delete all && ./node_modules/pm2/bin/pm2 start dist/bin/www.js",
    "node": "DEBUG=* node ./dist/bin/www.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-react-constant-elements",
      "@babel/plugin-transform-react-inline-elements"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.3.0",
    "@material-ui/icons": "^3.0.1",
    "amqplib": "^0.8.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "co": "^4.6.0",
    "colorbrewer": "^1.3.0",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "cron": "^1.4.1",
    "csvtojson": "^2.0.8",
    "d3": "^5.7.0",
    "debug": "^4.3.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.4",
    "fs": "0.0.1-security",
    "helmet": "^3.14.0",
    "http-errors": "~1.6.2",
    "immutability-helper": "^2.8.1",
    "jsonschema": "^1.2.4",
    "material-ui-chip-input": "^1.0.0-beta.8",
    "mathjs": "^7.6.0",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "mongodb": "^3.6.6",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nocache": "^2.1.0",
    "passport": "^0.4.0",
    "passport-ldapauth": "^2.1.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pm2": "^4.5.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.4",
    "react-select": "^2.1.0",
    "react-virtualized": "^9.21.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.1.1",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.6.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/plugin-transform-react-inline-elements": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.1",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.24.0",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.8.0",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^3.0.0"
  }
}
